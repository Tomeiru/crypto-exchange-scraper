generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  symbol String  @id @unique
  rates  Rates[]
}

model Rates {
  id           Int      @id @default(autoincrement())
  recordedTime DateTime @default(now())
  token        Token    @relation(fields: [tokenSymbol], references: [symbol])
  tokenSymbol  String
}
